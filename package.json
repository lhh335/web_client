    "angonsoft_textfield": "^2.0.7",
    "antd": "^2.9.0",
    "babel-polyfill": "^6.16.0",
    "chai": "^4.1.1",
    "chai-enzyme": "^0.8.0",
    "eslint-plugin-material-ui": "^1.0.0",
    "highcharts": "^5.0.11",
    "material-ui": "^0.16.2",
    "material-ui-codemod": "^0.1.5",
    "node-where": "^1.1.0",
    "qrcode-generator": "^1.1.0",
    "react-addons-perf": "^15.4.0",
    "react-bootstrap-table": "^3.1.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-data-grid": "^2.0.18",
    "react-data-grid-addons": "^2.0.18",
    "react-highcharts": "^12.0.0",
    "react-redux": "^4.4.6",
