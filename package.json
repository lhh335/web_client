{
  "name": "curtasoft-web-client",
  "version": "0.0.1",
  "description": "WebClient for Curta",
  "repository": {
    "type": "git",
    "url": "https://hantishoy@bitbucket.org/curtasoft/web_client.git"
  },
  "engines": {
    "node": ">=4"
  },
  "private": true,
  "scripts": {
    "release": "node ./script/release.js",
    "debug": "node ./script/debug.js",
    "test": "mocha --ui qunit --compilers js:babel-core/register test/test.js",
    "version": "node ./script/get-version.js",
    "start": "npm run browser:development",
    "crx": "crx pack build -p build.pem -o mycrx.crx",
    "build": "webpack --config webpack-production.config.js --progress --colors --profile",
    "browser:development": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --inline",
    "browser:build": "NODE_ENV=docs-production webpack --config webpack-production.config.js --progress --colors --profile",
    "browser:prd": "NODE_ENV=docs-production webpack-dev-server --config webpack-production.config.js --progress --colors",
    "gh-pages:build": "node ./gh-pages-build.js",
    "native:development": "node_modules/react-native/packager/packager.sh --reset-cache",
    "android:setup-port": "adb reverse tcp:8081 tcp:8081"
  },
  "peerDependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.0.0",
    "react-tap-event-plugin": "^2.0.0",
    "chai": "^3.0.0 || ^4.0.0",
    "cheerio": "0.19.x || 0.20.x || 0.22.x || 1.0.0-rc.1",
    "enzyme": "1.x || ^2.3.0"
  },
